# Server
PORT=3000
FRONTEND_URL=http://localhost:3001

# Database
MONGO_URI=mongodb://127.0.0.1:27017/your_database_name

# JWT
JWT_SECRET=your_jwt_secret
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# Redis
REDIS_URI=redis://:your_redis_password@127.0.0.1:6379
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Email
EMAIL_USER=your_email@example.com
EMAIL_PASS=your_email_password_or_app_specific_key

# OSS / Object Storage
OSS_REGION=your_oss_region
OSS_ACCESS_KEY_ID=your_oss_access_key_id
OSS_ACCESS_KEY_SECRET=your_oss_access_key_secret
OSS_BUCKET=your_oss_bucket_name
