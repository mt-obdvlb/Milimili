{
  "name": "@mtobdvlb/shared-types",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "barrels": "barrelsby -d src --delete --recurse -l all -q -S -H"
  },
  "dependencies": {
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "barrelsby": "^2.8.1"
  }
}